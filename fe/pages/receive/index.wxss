page {
  background-color: #F0F1F5;
}
.card_group {
  display: flex;
}
.card_group card {
  flex: 1 1 0;
}
.dot_box {
  position: relative;
  width: 56rpx;
  background-color: #fff;
  margin-bottom: 8rpx;
  display: flex;
  align-items: center;
}
.dot-off {
  width: 24rpx;
  height: 24rpx;
  border: 3rpx solid #DEDFE8;
  border-radius: 50%;
}
.dot-on {
  width: 24rpx;
  height: 24rpx;
  border: 3rpx solid #627AF8;
  background-color: #627AF8;
  border-radius: 50%;
}
.edit_header {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  width: 750rpx;
  height: 108rpx;
  border-bottom: 8rpx solid #F0F1F5;
  padding: 0 24rpx;
  background-color: #fff;
  font-size: 32rpx;
  color: #627AF8;
}
.edit_header .selected {
  color: #464646;
}
.space_top {
  height: 108rpx;
}

/* .to_move */
.to_move_group {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.to_move_group .cover {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: .4;
  background-color: #111111;
}
.to_move_group .to_collect {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 750rpx;
  height: 800rpx;
  border-radius: 8rpx 8rpx 0 0;
  background-color: #F0F1F5;
}
.to_move_group .header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 110rpx;
  padding: 0 32rpx;
}
.to_move_group .count {
  color: #464646;
  font-size: 32rpx;
}
.to_move_group .cancel {
  width: 24rpx;
  height: 24rpx;
}
.to_move_group .folderList {
  height: 580rpx;
  overflow: scroll;
}
.to_move_group .folder {
  display: flex;
  align-items: center;
  height: 132rpx;
  padding: 0 32rpx;
  background-color: #fff;
  margin-bottom: 8rpx;
}
.to_move_group .folder_icon {
  width: 67rpx;
  height: 60rpx;
  margin-right: 23rpx;
}
.to_move_group .info {
  flex: 1 1 0;
  display: flex;
  flex-direction: column;
}
.to_move_group .folder_name {
  font-size: 28rpx;
}
.to_move_group .folder_info {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 24rpx;
  color: #979797;
}
.to_move_group .folder_name {
  margin-bottom: 8rpx;
}
.to_move_group .footer {
  display: flex;
  align-items: center;
  padding: 0 32rpx;
  height: 110rpx;
}
.to_move_group .footer>image {
  width: 42rpx;
  height: 38rpx;
  margin-right: 16rpx;
}
.to_move_group .footer>text {
  font-size: 32rpx;
  color: #627AF8;
}

/* to_add_folder */
.add_folder_box {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}
.add_folder_box .cover{
  opacity: .4;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #111111;
}
.add_folder_box .add_folder{
  position: absolute;
  left: 110rpx;
  top: 500rpx;
  width: 530rpx;
  height: 370rpx;
  background-color: #fff;
  border-radius: 8rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.add_folder .title {
  font-weight: 700;
  font-size: 36rpx;
  display: flex;
  justify-content: center;
  margin-bottom: 40rpx;
}
.add_folder input {
  height: 75rpx;
  width: 450rpx;
  text-align: center;
  background-color: #F0F1F5;
  border-radius: 8rpx;
  margin-bottom: 40rpx;
  font-size: 32rpx;
}
.add_folder .func {
  width: 450rpx;
  padding-top: 20rpx;
  border-top: 3rpx solid #EEEFF3;
  display: flex;
  justify-content: space-around;
  font-size: 36rpx;
}
.add_folder .func .cancel {
  color: #464646;
  width: 200rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.add_folder .func .submit {
  color: #627AF8;
  width: 200rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* collect */
.collect {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100rpx;
  background-color: #627AF8;
}
.collect image {
  width: 32rpx;
  height: 30rpx;
  margin-right: 16rpx;
}
.collect text {
  font-size: 32rpx;
  color: #fff;
}